<h1>Talk to us!</h1>

<%= form_for [@user, @post] do |f| %>

<div>
    <%= f.label :title %>
    <%= f.text_field :title %>
</div>

<div>
    <%= f.label :date %>
    <%= f.text_field :date %>
</div>

<div>
    <%= f.label :time %>
    <%= f.text_field :time %>
</div>

<div>
    <%= f.label :body %>
    <%= f.text_field :body %>
</div>

<%= f.submit "Post" %>

<% end %>
